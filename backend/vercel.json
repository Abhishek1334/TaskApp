{
  "functions": {
    "api/tasks.ts": {
      "runtime": "@vercel/node"
    },
    "api/tasks/[id].ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/tasks",
      "destination": "/api/tasks"
    },
    {
      "source": "/tasks/(.*)",
      "destination": "/api/tasks/$1"
    }
  ]
} 